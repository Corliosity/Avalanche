function stands(){var e=Ti.Platform.osname,t="http://www.corliosity.com/json/standings.php",n=Ti.Network.createHTTPClient();n.onload=function(e){Ti.API.info("Calling the NHL Standings"),Ti.API.info("My Response "+this.responseText),jsonArray=Ti.App.fireEvent("get.stands",{responseText:this.responseText})},n.onerror=function(e){Ti.API.info("error, HTTP status = "+this.status),Ti.API.info(e.error),setTimeout(function(){alert("Sorry it appears that we are having difficulties getting the current events.  Please check back in a few minutes for updates.")},0)},n.open("GET",t),n.send()}module.exports=stands