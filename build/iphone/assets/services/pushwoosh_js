var PushWoosh={getToken:function(){return Ti.Network.remoteDeviceUUID},register:function(e,t){var n="POST",r=PushWoosh.getToken(),i=PushWoosh.baseurl+"registerDevice",s=new Date,o=s.getTimezoneOffset()*60,u={request:{application:PushWoosh.appCode,push_token:r,language:Ti.Platform.locale,hwid:Ti.Platform.id,timezone:o,device_type:1}};payload=u?JSON.stringify(u):"",Ti.API.info("sending registration with params "+payload),PushWoosh.helper(i,n,payload,e,t)},unregister:function(e,t){var n="POST",r=PushWoosh.getToken(),i=PushWoosh.baseurl+"unregisterDevice",s={request:{application:PushWoosh.appCode,hwid:Ti.Platform.id}};payload=s?JSON.stringify(s):"",Ti.API.info("sending registration with params "+payload),PushWoosh.helper(i,n,payload,e,t)},sendBadge:function(e,t,n){var r="POST",i=PushWoosh.getToken(),s=PushWoosh.baseurl+"setBadge",o={request:{application:PushWoosh.appCode,hwid:Ti.Platform.id,badge:e}};payload=o?JSON.stringify(o):"",Ti.API.info("sending badge with params "+payload),PushWoosh.helper(s,r,payload,t,n)},sendAppOpen:function(e,t){var n="POST",r=PushWoosh.getToken(),i=PushWoosh.baseurl+"applicationOpen",s={request:{application:PushWoosh.appCode,hwid:Ti.Platform.id}};payload=s?JSON.stringify(s):"",Ti.API.info("sending appOpen with params "+payload),PushWoosh.helper(i,n,payload,e,t)},sendPushStat:function(e,t,n){var r="POST",i=PushWoosh.getToken(),s=PushWoosh.baseurl+"pushStat",o={request:{application:PushWoosh.appCode,hwid:Ti.Platform.id,hash:e}};payload=o?JSON.stringify(o):"",Ti.API.info("sending pushStat with params "+payload),PushWoosh.helper(s,r,payload,t,n)},setTags:function(e,t,n){var r="POST",i=PushWoosh.getToken(),s=PushWoosh.baseurl+"setTags",o={request:{application:PushWoosh.appCode,hwid:Ti.Platform.id,tags:e}};payload=o?JSON.stringify(o):"",Ti.API.info("sending tags with params "+payload),PushWoosh.helper(s,r,payload,t,n)},helper:function(e,t,n,r,i){var s=Ti.Network.createHTTPClient();s.setTimeout(6e4),s.onerror=function(e){i(this,e)},s.onload=functio