function MasterView(){var e=Ti.Platform.osname,t=Ti.UI.createView({backgroundImage:"images/bg.png",backgroundColor:"transparent"}),n=Ti.UI.createTableView({backgroundColor:"transparent"});return t.add(n),n.addEventListener("click",function(e){t.fireEvent("itemSelected",{link:e.row.link,title:e.row.myTitle,pubDate:e.row.myDate,description:e.row.myDescrip,myImage:e.row.myImage})}),t.refreshRssTable=function(t){if(Object.prototype.toString.apply(t)==="[object Array]"){var r=[];for(var i=0;i<t.length;i++){if(e=="android")var s=i%2?"#eaeaea":"#999";else var s="transparent";r.push(createRssRow(t[i],s))}n.setData(r)}},t}var createRssRow=function(e,t){var n=Ti.UI.createTableViewRow({height:70,link:e.link,myTitle:e.title,myDate:e.pubDate,myDescrip:e.descript,myImage:e.image==undefined?"":e.image,backgroundColor:t,className:"itemRow",hasChild:!0}),r=Ti.UI.createImageView({image:e.image,height:42,width:68,left:5,top:3}),i=Ti.UI.createLabel({text:e.title,color:"#000",height:70,font:{fontSize:16},left:83,right:5}),s=Ti.UI.createLabel({text:e.pubDate,textAlign:"center",color:"#444",font:{fontSize:12},height:"auto",width:68,left:5,bottom:3});return n.add(r),n.add(s),n.add(i),n};module.exports=MasterView