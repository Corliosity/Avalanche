function ApplicationWindow(e){function l(){t.loadRssFeed({success:function(e){s.refreshRssTable(e)}})}var t=require("services/NHLrss"),n=require("ui/nhl/MasterView"),r=require("ui/nhl/DetailView"),i=Ti.UI.createWindow({backgroundColor:"transparent",title:"NHL News",backButtonTitle:"Back",barColor:"#13386c"}),s=new n,o=new r,u=Ti.UI.createWindow({title:"RSS Reader"}),a=Ti.UI.createButton({systemButton:Ti.UI.iPhone.SystemButton.REFRESH});a.addEventListener("click",function(e){l()}),i.rightNavButton=a,i.add(s);var f=Ti.UI.createWindow({backgroundColor:"#eaeaea",title:"NHL News",barColor:"#13386c"});return f.add(o),s.addEventListener("itemSelected",function(t){o.showLink(t.link),o.showArticle(t.description),o.showTitle(t.title),o.showDate(t.pubDate),e.open(f)}),l(),i}module.exports=ApplicationWindow