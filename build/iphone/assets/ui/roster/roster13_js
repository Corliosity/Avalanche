function roster13(e){var t=require("services/roster"),n=new t,r=Ti.UI.createWindow({title:"2013 Roster",backgroundColor:"transparent",barColor:"#13386c",layout:"vertical",backButtonTitle:"Back"}),i=[],s=Ti.UI.createTableView(),o=function(e){var t=e.player,n=e.number,r=e.position,i=e.shoots,s=e.season,o=e.injured,u=e.img;Ti.API.info(u);var a=Ti.UI.createTableViewRow({height:"60dp",hasChild:!0,thisTitle:t,thisNum:n,position:r,shoots:i,stat:s,playImg:u}),f=Ti.UI.createImageView({image:"http://www.corliosity.com/json/"+u,height:50,width:40,left:"0dp"}),l=Ti.UI.createLabel({text:t,width:"auto",height:"auto",textAlign:"left",top:"3dp",left:"75dp",color:"black",font:{fontWeight:"bold",fontSize:18}}),c=Ti.UI.createLabel({text:r=="Goaltender"?r+" Catches: "+i:r+"  Shoots: "+i,width:"auto",height:"auto",textAlign:"left",top:"20dp",left:"50dp",color:"black",font:{fontSize:14}}),h=Ti.UI.createLabel({text:n,width:"auto",height:"auto",textAlign:"left",top:"2dp",left:"50dp",color:"black",font:{fontSize:17,fontWeight:"bold"}}),p=Ti.UI.createLabel({text:"Height: "+e.height+" Weight: "+e.weight,width:"auto",height:"auto",textAlign:"left",top:"40dp",left:"75dp",color:"black",font:{fontSize:14}}),d=Ti.UI.createLabel({text:o===!0?"Injured":"",width:"auto",height:"auto",textAlign:"left",top:"20dp",left:"240dp",color:"black",font:{fontSize:12}});return a.add(l),a.add(f),a.add(c),a.add(h),a.add(p),a.add(d),a.thisTitle=t,a.thisDetail=n,a.className="custom_row",a};return Ti.App.addEventListener("get.roster",function(e){var t=JSON.parse(e.responseText),n=0,r=t.length,i="",u,a=[],f="";for(n=0;n<r;n+=1)f=n%2?"#fff":"#ccc",u=o(t[n]),i!=t[n].position&&(i=t[n].position,u.header=t[n].position),u.setBackgroundColor(f),a.push(u);s.setData(a)}),s.addEventListener("click",function(t){var n=require("/ui/roster/rosterDetail");e.open(new n(e,t.row.playImg,t.row.thisTitle,t.row.thisNum,t.row.position,t.row.shoots,t.row.stat))}),r.add(s),r}module.exports=roster13