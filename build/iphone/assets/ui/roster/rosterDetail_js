function rosterDetail(e,t,n,r,i,s,o){var u=Ti.Platform.osname,a=Ti.UI.createWindow({title:n,backButtonTitle:"Roster",backgroundColor:"transparent",backgroundImage:"images/avalanche_team.png",barColor:"#13386c"}),f=Ti.UI.createScrollView({contentwidth:"auto",color:"#000",contentHeight:"auto",width:"90%",height:"90%",showHorizontalScrollIndicator:!0,backgroundColor:"#fff",opacity:.75,borderRadius:"5px"});Ti.API.info("My Data is "+o.gp);var l=n.split(" "),c=Ti.UI.createImageView({image:"http://www.corliosity.com/json/"+t,width:100,height:150,right:"3dp",top:"3dp"});f.add(c);var h=Ti.UI.createLabel({text:l[0]+"\n"+l[1],top:"3dp",left:"3dp",height:"auto",width:"auto",color:"#000",font:{fontSize:18}});f.add(h);var p=Ti.UI.createLabel({text:r,top:"40dp",left:"3dp",width:"auto",color:"#000",height:"auto",font:{fontSize:15}});f.add(p);var d=Ti.UI.createLabel({text:i=="Goaltender"?"Position: "+i+"\n"+"Catches: "+s:"Position: "+i+"\n"+"Shoots: "+s,top:"60dp",left:"3dp",height:"auto",width:"auto",color:"#000",font:{fontSize:15}});f.add(d);var v=Ti.UI.createLabel({text:"GP",top:"155dp",left:"3dp",height:"auto",width:"auto",color:"#000",font:{fontSize:14}});f.add(v);var m=parseInt(o.goals)+parseInt(o.assist),g=parseInt(o.goals)/parseInt(o.shots)*100,y=Ti.UI.createLabel({text:o.gp,top:"168dp",left:"3dp",height:"auto",width:"auto",color:"#000",font:{fontSize:14}});f.add(y);var b=Ti.UI.createLabel({text:i=="Goaltender"?"S":"G",top:"155dp",left:"35dp",height:"auto",width:"auto",color:"#000",font:{fontSize:14}}),w=Ti.UI.createLabel({text:i=="Goaltender"?o.saves:o.goals,top:"168dp",left:"35dp",height:"auto",width:"auto",color:"#000",font:{fontSize:14}}),E=Ti.UI.createLabel({text:i=="Goaltender"?"SA":"A",top:"155dp",left:"65dp",height:"auto",width:"auto",color:"#000",font:{fontSize:14}}),S=Ti.UI.createLabel({text:i=="Goaltender"?o.shots_against:o.assist,top:"168dp",left:"65dp",height:"auto",width:"auto",color:"#000",font:{fontSize:14}}),x=Ti.UI.createLabel({text:i=="Goaltender"?"S %":"P