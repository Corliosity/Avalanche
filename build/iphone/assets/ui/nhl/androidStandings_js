function androidStandings(e){var t=Ti.UI.createWindow({title:"NHL Standings",backgroundImage:"images/nhl_bg.png",backgroundColor:"transparent",barColor:"#13386c",backButtonTitle:"Back"}),n=require("services/stands"),r=new n,i=Ti.UI.createTableView();t.add(i);var s=function(e){var t=parseInt(e.points),n=Ti.UI.createTableViewRow({backgroundColor:"#fff",height:"50dp",width:Ti.UI.SIZE}),r=Ti.UI.createLabel({color:"#000",font:{fontSize:16,fontWeight:"bold"},height:Ti.UI.SIZE,highlightedColor:"#fff",left:"55dp",top:"0dp",text:e.team,width:Ti.UI.SIZE}),i=Ti.UI.createLabel({color:"#666",font:{fontSize:14},height:Ti.UI.SIZE,highlightedColor:"#ddd",left:"55dp",top:"20dp",text:"Points: "+t.toFixed(0),width:Ti.UI.SIZE}),s=Ti.UI.createImageView({image:"http://www.corliosity.com/json/"+e.img,left:"0dp",top:"0dp",height:50,width:50}),o=Ti.UI.createLabel({color:"#666",font:{fontSize:14},height:Ti.UI.SIZE,left:"125dp",top:"20dp",highlightedColor:"#ddd",text:"W: "+e.wins+" L: "+e.losses+" OT: "+e.overtime,width:Ti.UI.SIZE}),u=Ti.UI.createLabel({color:"#666",font:{fontSize:14},height:Ti.UI.SIZE,left:"240dp",top:"20dp",highlightedColor:"#ddd",text:"STK: "+e.streak,width:Ti.UI.SIZE});return n.add(r),n.add(i),n.add(s),n.add(o),n.add(u),n};return Ti.App.addEventListener("get.sortedStands",function(e){var t=JSON.parse(e.data),n=0,r=t.length,o="",u,a=[],f="";for(n=0;n<r;n+=1)f=n%2?"#fff":"#ccc",u=s(t[n]),o!=t[n].division&&(o=t[n].division,u.header=t[n].division),u.setBackgroundColor(f),a.push(u);i.setData(a)}),Ti.App.addEventListener("get.stands",function(e){function n(e){return function(t,n){return Ti.API.info(t[e]+"  is A and B is  "+n[e]),t[e]>n[e]?-1:t[e]<n[e]?1:0}}var t=JSON.parse(e.responseText);Ti.App.fireEvent("get.sortedStands",{data:JSON.stringify(t)})}),t}module.exports=androidStandings