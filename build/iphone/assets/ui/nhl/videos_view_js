function videos_view(e,t,n,r){var i=Ti.Platform.osname,s=Ti.UI.createWindow({title:t,backButtonTitle:"Back",backgroundColor:"#333",barColor:"#13386c"});if(i=="iphone"){Ti.API.info(n);var o=Ti.Media.createVideoPlayer({url:n,mediaControlStyle:Ti.Media.VIDEO_CONTROL_DEFAULT,scalingMode:Ti.Media.VIDEO_SCALING_MODE_FILL,fullscreen:!1,autoplay:!0});s.add(o)}else{var u=n,a=Ti.UI.createButton({title:"Start Video",top:"0dp",height:"40dp",left:"10dp",right:"10dp"}),f=Ti.UI.createButton({title:r,top:"40dp",height:Ti.UI.SIZE,width:Ti.UI.SIZE});a.addEventListener("click",function(){var e=Ti.Media.createVideoPlayer({url:u,backgroundColor:"blue",movieControlMode:Ti.Media.VIDEO_CONTROL_DEFAULT,scalingMode:Ti.Media.VIDEO_SCALING_ASPECT_FILL,autoplay:!0}),t=Ti.UI.createButton({title:"Exit Video",top:"0dp",height:"40dp",left:"10dp",right:"10dp"});t.addEventListener("click",function(){e.hide(),e.release(),e=null}),e.add(t),s.add(e)}),s.add(a),s.add(f)}return s}module.exports=videos_view