function MasterView(){var e=Ti.UI.createView({backgroundColor:"#eaeaea",backgroundImage:"images/nhl_bg.png"}),t=Ti.UI.createTableView({backgroundColor:"transparent"});return e.add(t),t.addEventListener("click",function(t){e.fireEvent("itemSelected",{link:t.row.link,title:t.row.myTitle,pubDate:t.row.myDate,description:t.row.myDescrip})}),e.refreshRssTable=function(e){if(Object.prototype.toString.apply(e)==="[object Array]"){var n=[];for(var r=0;r<e.length;r++)n.push(createRssRow(e[r]));t.setData(n)}},e}var createRssRow=function(e){var t=Ti.UI.createTableViewRow({height:70,link:e.link,myTitle:e.title,myDate:e.pubDate,myDescrip:e.descript,className:"itemRow",hasChild:!0}),n=Ti.UI.createImageView({image:e.image,height:42,width:68,left:5,top:3}),r=Ti.UI.createLabel({text:e.title,color:"#000",height:70,font:{fontSize:16},left:83,right:5}),i=Ti.UI.createLabel({text:e.pubDate,textAlign:"center",color:"#444",font:{fontSize:12},height:"auto",width:68,left:5,bottom:3});return t.add(n),t.add(i),t.add(r),t};module.exports=MasterView