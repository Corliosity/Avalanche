function videos(e){var t=require("services/avs_videos"),n=new t,r=Ti.Platform.osname,i=Ti.UI.createWindow({title:"Videos",backButtonTitle:"Back",backgroundColor:"#13386c",barColor:"#13386c"}),s=[],o=Ti.UI.createTableView(),u=function(e){var t=e.title,n=e.src,r=e.thumbnail_link,i=e.description,s=Ti.UI.createTableViewRow({height:"60dp",hasChild:!0,thisTitle:t,playImg:r,video:n,des:i}),o=Ti.UI.createImageView({image:r,width:50,height:50,left:"0dp"}),u=Ti.UI.createLabel({text:t==""?"Avalanche video":t,width:"auto",height:"auto",textAlign:"left",top:"3dp",left:"75dp",color:"black",font:{fontWeight:"bold",fontSize:14}}),a=Ti.UI.createLabel({text:i,width:"auto",height:"auto",textAlign:"left",top:"2dp",left:"50dp",color:"black",font:{fontSize:17,fontWeight:"bold"}});return s.add(u),s.add(o),s.className="custom_row",s};return Ti.App.addEventListener("get.videos",function(e){var t=JSON.parse(e.responseText);s=[];for(var n=0;n<t.length;n++)bgcolor=n%2?"#fff":"#ccc",row=u(t[n]),row.setBackgroundColor(bgcolor),s.push(row);o.setData(s)}),o.addEventListener("click",function(t){if(r=="iphone"){var n=require("/ui/nhl/videos_view");e.open(new n(e,t.row.thisTitle,t.row.video,t.row.des))}else{var s=Ti.Media.createVideoPlayer({url:t.row.video,backgroundColor:"blue",movieControlMode:Ti.Media.VIDEO_CONTROL_DEFAULT,scalingMode:Ti.Media.VIDEO_SCALING_ASPECT_FILL,autoplay:!0}),o=Ti.UI.createButton({title:"Exit Video",top:"0dp",height:"40dp",left:"10dp",right:"10dp"});o.addEventListener("click",function(){s.hide(),s.release(),s=null}),s.add(o),i.add(s)}}),i.add(o),i}module.exports=videos