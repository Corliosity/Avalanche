function responseTweet(e,t,n,r,i,s){var o=Ti.UI.createWindow({title:e,backButtonTitle:"Back",backgroundColor:"#999",barColor:"#4099FF"});Ti.API.info(i);var u=Ti.UI.createView({borderRadius:6,borderWidth:1,borderColor:"#000",top:"5dp",width:"100%",height:Ti.UI.SIZE,backgroundColor:"#fff"}),a=Ti.UI.createWebView({url:"http://twitter.com/"+e+"/status/"+i});return u.add(a),o.add(u),o}module.exports=responseTweet